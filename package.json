{
  "name": "albion-online-bot",
  "version": "1.0.0",
  "description": "An Albion Online Discord Bot for getting information from the AO Public API.  Also a Killboard Bot.",
  "main": "dist/server.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/jubairsaidi/albion-online-bot.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/jubairsaidi/albion-online-bot/issues"
  },
  "homepage": "https://github.com/jubairsaidi/albion-online-bot",
  "dependencies": {
    "axios": "^0.19.2",
    "benchlogga": "^1.0.3",
    "discord.js": "^12.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/eslint": "^6.8.0",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@types/prettier": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "dependency-cruiser": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "lint": "eslint '*/**/*.{js,ts}'",
    "deps:check": "depcruise --validate .dependency-cruiser.json src",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "serve": "tsc-watch ./src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\" --onFailure \"echo Beep! Compilation Failed\" --compiler typescript/bin/tsc",
    "build": "tsc",
    "start": "node dist/server.js"
  },
